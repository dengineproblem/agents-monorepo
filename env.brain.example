# OpenAI API Key
OPENAI_API_KEY=your-openai-api-key-here

# Supabase Configuration
SUPABASE_URL=your-supabase-url
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Agent Brain Configuration
BRAIN_MODEL=gpt-5
BRAIN_PORT=7080
BRAIN_DRY_RUN=false
BRAIN_DEBUG_LLM=false
BRAIN_MAX_ACTIONS_PER_RUN=5
BRAIN_TEST_MODE=false

# Scoring Agent Configuration
SCORING_ENABLED=true
SCORING_MIN_IMPRESSIONS=1000
SCORING_PREDICTION_DAYS=3
SCORING_MODEL=gpt-5

# Agent Service URL
AGENT_SERVICE_URL=http://agent-service:8080

# Cron Configuration
CRON_ENABLED=true
CRON_SCHEDULE="0 8 * * *"

# Telegram (optional fallback)
TELEGRAM_FALLBACK_BOT_TOKEN=your-telegram-bot-token
